#include<iostream>
using namespace std;

int main()
{ int size;
  cin>>size;
  int arr[size],max,min,i;
  cout<<"Enter elements of array: ";
  for(i=0;i<size;i++)
  cin>>arr[i];

  cout<<"Your array is: ";
  for(i=0;i<size;i++)
  cout<<arr[i]<<"  ";
  cout<<endl;

  max=arr[0];
  min=arr[0];
  for(i=0;i<size;i++)
  if(max<arr[i])
   max=arr[i];
  else if(min>arr[i])
   min=arr[i];

  cout<<"Maximum element of Array: "<<max<<endl;
  cout<<"Minimum element of Array: "<<min<<endl;
  return 0;
}
